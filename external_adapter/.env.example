# External Adapter Configuration
EA_PORT=8080
API_KEY=your_api_key_here
CORS_ORIGIN=*

# Weather API Configuration
OPENWEATHER_API_KEY=your_openweather_api_key_here

# IPFS Configuration
IPFS_URL=https://your-ipfs-endpoint.com/api/v0
IPFS_AUTH_TOKEN=your_ipfs_auth_token_here

# HashiCorp Vault Configuration
VAULT_ENDPOINT=http://host.docker.internal:8200
VAULT_TOKEN=your_vault_token_here
VAULT_ENCRYPTION_KEY=weather-data
VAULT_SECRET_PATH=secret
VAULT_ENABLED=true
VAULT_KV_MOUNT=secret